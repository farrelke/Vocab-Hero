{
  "manifest_version": 2,

  "key": "",
  "name": "Vocab 英雄",
  "short_name": "Vocab Hero",
  "description": "Learn new Chinese words every time you open new tab. ",
  "version": "0.0.17",

  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },

  "icons": {
    "16": "vocabulary-small.png",
    "48": "vocabulary-medium.png",
    "128": "vocabulary-big.png"
  },

  "browser_action": {
    "default_icon": "vocabulary-small.png"
  },

  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "webNavigation",
    "history",
    "http://localhost:3000/*",
    "*://*.jisho.org/*",
    "*://*.reddit.com/*"
  ],

  "externally_connectable": {
    "matches": ["http://localhost:3000/*", "http://localhost:8080/*", "*://*.subtitlehero.com/*"]
  },

  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
