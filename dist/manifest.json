{
  "manifest_version": 2,

  "key": "",
  "name":  "Vocab 英雄",
  "description": "Learn new Chinese words every time you open new tab. ",
  "version": "0.0.8",

  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },

  "icons": {
    "16": "vocabulary-small.png",
    "48": "vocabulary-medium.png",
    "128": "vocabulary-big.png"
  },

  "browser_action": {
    "default_icon": "vocabulary-small.png"
  },

  "permissions": [
    "storage",
    "unlimitedStorage"
  ],

  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
